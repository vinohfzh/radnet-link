datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Link {
  id          String   @id @default(cuid())
  originalUrl String
  shortCode   String   @unique
  clicks      Int      @default(0)
  isActive    Boolean  @default(true)
  createdAt  DateTime @default(now())
}
